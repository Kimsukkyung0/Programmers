class Solution {
    public int solution(int a, int b) {
       String strA = Integer.toString(a);
       String strB = Integer.toString(b);
        int answer = Integer.parseInt(strA+strB);
        int aws = Integer.parseInt(strB+strA);
   if( answer>aws){ return answer;}else if(answer==aws){return answer;}
        else{return aws;}
    }
}
